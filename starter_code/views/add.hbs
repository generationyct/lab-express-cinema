

{{#each movies}}
<div class="movie-list">
  <h2>Movie title: {{ this.title }} </h2>
</div>
<div class="movie-list">
  <p>Directed by: {{ director }} </p>
</div>
<div class="movie-list">
  <p>Stars: {{ stars }} </p>
</div>
<div class="movie-list">
  <p>Showtimes: {{ showtimes }} </p>
</div>
<div class="movie-list">
  <img src="{{ image }}" alt="movie image">
</div>


<br>

{{/each}}

